{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "rootDir": "./src",
    "outDir": "./dist",
  }
}
